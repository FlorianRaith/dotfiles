<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add Row">
        <Item>
          <IdeaActionID>Console.TableResult.AddRow</IdeaActionID>
          <ShortCut>'Alt+Insert'</ShortCut>
          <Description>Add Row</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Class...">
        <Item>
          <IdeaActionID>GotoClass</IdeaActionID>
          <ShortCut>'Ctrl+N'</ShortCut>
          <Description>Class...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete Rows">
        <Item>
          <IdeaActionID>Console.TableResult.DeleteRows</IdeaActionID>
          <ShortCut>'Ctrl+Y'</ShortCut>
          <Description>Delete Rows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Edit Source">
        <Item>
          <IdeaActionID>Vcs.EditSource</IdeaActionID>
          <ShortCut>'F4'</ShortCut>
          <Description>Edit Source</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Execute">
        <Item>
          <IdeaActionID>Console.Jdbc.Execute</IdeaActionID>
          <ShortCut>'Ctrl+Enter'</ShortCut>
          <Description>Execute</Description>
          <Count>1</Count>
          <Hit>1</Hit>
        </Item>
      </Statistic>
      <Statistic Action="File">
        <Item>
          <IdeaActionID>NewFile</IdeaActionID>
          <ShortCut>'Ctrl+Shift+N'</ShortCut>
          <Description>File</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate DDL to Clipboard">
        <Item>
          <IdeaActionID>DatabaseView.CopyDdlAction</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+G' or 'Ctrl+Shift+C'</ShortCut>
          <Description>Generate DDL to Clipboard</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Go to Declaration or Usages">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Go to Declaration or Usages</Description>
          <Count>110</Count>
          <Hit>1</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Query Console">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.New.Generate</IdeaActionID>
          <ShortCut>'Ctrl+Shift+Q'</ShortCut>
          <Description>Query Console</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>DatabaseView.Refresh</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Refresh</Description>
          <Count>1</Count>
          <Hit>1</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>2</Count>
          <Hit>5</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Show Context Actions">
        <Item>
          <IdeaActionID>ShowIntentionActions</IdeaActionID>
          <ShortCut>'Alt+Enter'</ShortCut>
          <Description>Show Context Actions</Description>
          <Count>1</Count>
          <Hit>40</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Show Local Changes as UML">
        <Item>
          <IdeaActionID>UML.ShowChanges</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+D'</ShortCut>
          <Description>Show Local Changes as UML</Description>
          <Count>1</Count>
          <Hit>19</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Show Visualization Popup...">
        <Item>
          <IdeaActionID>ShowUmlDiagramPopup</IdeaActionID>
          <ShortCut>'Ctrl+Alt+U'</ShortCut>
          <Description>Show Visualization Popup...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Visualization...">
        <Item>
          <IdeaActionID>ShowUmlDiagram</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+U'</ShortCut>
          <Description>Show Visualization...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>